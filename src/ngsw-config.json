{
    "index": "/index.html",
    "assetGroups": [
      { 
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html",
            "/manifest.webmanifest",
            "/*.css",
            "/*.js"
          ]
        }
      },
      {
        "name": "assets",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**"
          ]
        }
      }
    ],
    "dataGroups": [
      { 
        "name": "api",
        "urls": [
           "https://sso.heterohealthcare.com/iconnect/**"
        ],  
        "cacheConfig": {
          "maxSize": 10,
          "maxAge": "1h",
          "timeout": "5s",
          "strategy": "performance"
        }
      }
    ]
  }
  